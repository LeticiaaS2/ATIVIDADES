programa
{
	
	funcao inicio()
	{
  /*
  LETICIA PEREIRA E CAMILA BRINGEL
  */
		real n1,n2,n3,media

		escreva("Informe a primeira nota: ")
		leia(n1)
		escreva("Informe a segunda nota: ")
		leia(n2)
		escreva("Informe a terceira nota: ")
		leia(n3)

		media=(n1+n2+n3)/3
		se((media>=6)e(media<=8)){
			escreva("Boa nota")
			
		}senao se(media>8){
			escreva("Excelente nota")
			
		}senao{
			escreva("Precisa melhorar")
		}
	}
}
